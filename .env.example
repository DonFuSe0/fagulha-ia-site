# Rename this file to `.env` and fill in the appropriate values.
#
# Supabase configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# The public URL of your deployed site (used for OAuth redirects)
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# ComfyUI API (internal).  Provide the base URL for your ComfyUI instance.
COMFYUI_API_URL=http://localhost:8188
# Secret used to validate webhooks from ComfyUI
COMFYUI_WEBHOOK_SECRET=change-me

# Stripe (optional) - if you enable payments
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
